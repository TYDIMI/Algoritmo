programa {
  funcao inicio() {
    inteiro idd
    escreva("Insira sua idade:")
    leia(idd)
    se(idd < 10) {
    escreva("Você poderá ver filmes de classificação Livre")
    }
    senao se(idd >= 10 e idd <14) {
      escreva("Você poderá ver filmes de classificação Livre e Juvenil")
    }
      senao {
        escreva("Você poderá ver filmes de classificação Livre, Juvenil e Adulto")
      }
    }
  }
}
